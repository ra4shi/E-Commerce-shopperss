                    <!-- General elements -->
                   
                        <form action="" method="post" enctype="multipart/form-data">
                            <label class="form-label">
                                <span>Name</span><br>
                                <input required class="form-input dark:text-gray-300 dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:focus:shadow-outline-gray" type="text" name="name" />
                              </label>
                           
                            <label class="block mt-4 text-sm">
                              <span class="text-gray-700 dark:text-gray-400">Category</span>
                              <select name="Category" class="block w-full mt-1 text-sm form-select dark:text-gray-300 dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:focus:shadow-outline-gray">
                                
                                
                                <% for(let i=0; i<Category.length; i++){ %>
                                <option value="<%= Category[i].Category %>"><%= Category[i].Category %></option>
                                <% } %>
                              </select>
                            </label><br>
                            <label class="block text-sm">
                                <span class="text-gray-700 dark:text-gray-400">Price</span>
                                <input class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input" type="number" name="price" />
                              </label>
                              <br>
                            
                            <label class="block text-sm">
                              <span class="text-gray-700 dark:text-gray-400">Image</span>
                              <input accept="image/*" class="block w-full mt-1 text-sm form-input dark:text-gray-300 dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:focus:shadow-outline-gray" type="file" name="image" multiple />
                            </label>
                            <br>
                            
                            <label class="block mt-4 text-sm">
                                <span class="text-gray-700 dark:text-gray-400">Description</span>
                                <textarea name="description" class="block w-full mt-1 text-sm form-textarea dark:text-gray-300 dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:focus:shadow-outline-gray" rows="2" placeholder="Enter some long form content."></textarea>
                              </label>
                              <br>
                              <br>
                              
                           
                            <label class="block text-sm">
                              <span class="text-gray-700 dark:text-gray-400">Color</span>
                              <input class="block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-purple-400 focus:outline-none focus:shadow-outline-purple dark:text-gray-300 dark:focus:shadow-outline-gray form-input" type="text" name="color" />
                            </label>
                          <br>

                          <br>
                            <br>
