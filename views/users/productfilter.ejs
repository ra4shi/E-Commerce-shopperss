	<script>
		function show(id){
			$.ajax({
				url:"/addToCart",
				method:"post",
				data:{
					query:id
				},
				success:(response)=>{
					if(response.success){
						Swal.fire({
						position: 'center',
						icon: 'success',
						title: 'Product added to cart',
						showConfirmButton: false,
						timer: 1500
					})
					}
				}
			})
		}

		function filter(start,end){
			$.ajax({
				url:'/priceFilter',
				data:{
					start:start,
					end:end
				},
				method:"post",
				success:(response)=>{
					
				}
			})
		}
	</script>

